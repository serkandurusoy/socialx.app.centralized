  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "preset": "jest-preset-typescript-react-native",
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/App/**/*.test.ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/__tests__/index.d.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "rnpm": {
    "assets": [
      "./Internals/theme/assets/fonts"
    ]
  }
